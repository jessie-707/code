{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["es5", "es6", "dom", "dom.iterable"],
    "module": "commonjs",
    "jsx": "react",
    "moduleResolution": "node",
    "strict": true,
    "noEmit": true,
    "allowJs": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "esModuleInterop": true
  },
  "include": ["src", "index.d.ts"]
}
